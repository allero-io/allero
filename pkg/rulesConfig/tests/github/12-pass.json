{
    "dummy": {
     "ownerName": "dummy",
     "ownerType": "User",
     "id": 0,
     "repositories": {
      "dummy": {
       "name": "dummy",
       "fullName": "dummy/dummy",
       "id": 0,
       "programmingLanguages": [
        "Java"
       ],
       "github-actions-workflows": {
        "pr[ESCAPED_DOT]yml": {
         "relativePath": ".github/workflows/pr.yml",
         "filename": "pr.yml",
         "origin": "github_actions",
         "content": {
          "jobs": {
           "build": {
            "name": "Build and verify skill handler",
            "needs": "lint",
            "permissions": {
             "checks": "write",
             "pull-requests": "write"
            },
            "runs-on": "ubuntu-latest",
            "steps": [
             {
              "name": "Checkout sources",
              "uses": "actions/checkout@v3"
             },
             {
              "name": "Setup Java 11",
              "uses": "actions/setup-java@v3.5.1",
              "with": {
               "cache": "maven",
               "distribution": "adopt",
               "java-version": 11
              }
             },
             {
              "name": "Verify and create coverage reports",
              "run": "mvn verify -Pits,cov -B -ff",
              "working-directory": "./lambda"
             },
             {
              "name": "Push coverage report",
              "uses": "codecov/codecov-action@v3",
              "with": {
               "files": "./lambda/target/site/jacoco/jacoco.xml"
              }
             }
            ]
           },
           "lint": {
            "name": "Lint project",
            "permissions": {
             "pull-requests": "write"
            },
            "runs-on": "ubuntu-latest",
            "steps": [
             {
              "name": "Checkout sources",
              "uses": "actions/checkout@v3",
              "with": {
               "fetch-depth": 0
              }
             },
             {
              "env": {
               "GITHUB_TOKEN": "${{ secrets.GITHUB_TOKEN }}",
               "IGNORE_GENERATED_FILES": true,
               "IGNORE_GITIGNORED_FILES": true,
               "VALIDATE_ALL_CODEBASE": false,
               "VALIDATE_EDITORCONFIG": true,
               "VALIDATE_GITHUB_ACTIONS": true,
               "VALIDATE_JAVA": true,
               "VALIDATE_JSON": true,
               "VALIDATE_MARKDOWN": true,
               "VALIDATE_XML": true,
               "VALIDATE_YAML": true
              },
              "name": "Lint code base",
              "uses": "docker://ghcr.io/github/super-linter:slim-latest"
             }
            ]
           }
          },
          "name": "Pull request build",
          "true": {
           "pull_request": {
            "branches": [
             "master"
            ]
           }
          }
         }
        }
       },
       "jfrog-pipelines": {}
      }
     }
    }
   }
   